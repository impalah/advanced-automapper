{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File - PyAutomapper",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Pytest - PyAutomapper",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "cwd": "${workspaceRoot}/",
      "args": ["tests"],
      "justMyCode": false
    },
    {
      "name": "MyPy - PyAutomapper",
      "type": "debugpy",
      "request": "launch",
      "module": "mypy",
      "cwd": "${workspaceRoot}/",
      "args": ["--ignore-missing-imports", "src/"]
    }
  ]
}
